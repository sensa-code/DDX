// ======= HELPERS =======
function resolveSearch(query) {
  const q = query.toLowerCase().trim();
  for (const [slang, medical] of Object.entries(SYNONYMS)) {
    if (q === slang.toLowerCase() || q.includes(slang.toLowerCase())) return medical.toLowerCase();
  }
  return q;
}

const TIER1_KEYWORDS = ["CBC","Xå…‰","ç†å­¸æª¢æŸ¥","å°¿æ¶²åˆ†æ","è¡€ç³–","é›»è§£è³ª","è¡€å£“","çœ¼å£“","è§¸è¨º","é«”æº«","PCV","ç³ä¾¿","å¿ƒé›»åœ–","å°¿æ¯”é‡","BUN","Cre","SDMA","è¡€æŠ¹ç‰‡","ECG","é£²é£Ÿå²","ç”¨è—¥å²","æš´éœ²å²","æ’é™¤","æœç³–èƒº","fPLI","cPLI","SNAP","4Dx","ä¹³é…¸","NT-proBNP","è¡€æ°£","ç–¼ç—›è©•ä¼°","ç¥ç¶“å­¸æª¢æŸ¥","FAST scan","è€³é¡","è€³å¢","å£è…”æª¢æŸ¥","è‡¨åºŠè©•ä¼°","å“ç¨®è©•ä¼°"];
function isTier1(w) { return TIER1_KEYWORDS.some(k => w.includes(k)); }

