function runValidation() {
  console.log("=== è¤‡åˆæŸ¥è©¢ç³»çµ±é©—è­‰ ===");
  console.log(`æ¸¬è©¦æ¡ˆä¾‹ç¸½æ•¸: ${TEST_CASES.length}`);

  let passed = 0;
  let failed = 0;
  const failedCases = [];
  const details = [];

  TEST_CASES.forEach(tc => {
    // è¨­å®šæ¸¬è©¦ç‹€æ…‹
    state.compositeSymptoms = tc.symptoms.filter(s => findSymptom(s));
    state.compositeLabs = tc.labs.filter(l => LAB_DB.abnormalities.find(x => x.id === l));

    // åŸ·è¡ŒæŸ¥è©¢
    const results = compositeQuery();
    const top10 = results.slice(0, 10).map(r => r.en.toLowerCase());

    // æª¢æŸ¥æ˜¯å¦åŒ¹é…
    const detected = tc.expectedMatches.some(exp =>
      top10.some(r => r.includes(exp.toLowerCase()))
    );

    if (detected) {
      passed++;
      details.push({
        id: tc.id,
        name: tc.name,
        status: "âœ… PASS",
        top3: top10.slice(0, 3)
      });
    } else {
      failed++;
      failedCases.push({
        id: tc.id,
        name: tc.name,
        expected: tc.expectedMatches,
        actualTop5: top10.slice(0, 5),
        symptoms: tc.symptoms,
        labs: tc.labs
      });
      details.push({
        id: tc.id,
        name: tc.name,
        status: "âŒ FAIL",
        top3: top10.slice(0, 3),
        expected: tc.expectedMatches
      });
    }
  });

  // æ¸…é™¤æ¸¬è©¦ç‹€æ…‹
  state.compositeSymptoms = [];
  state.compositeLabs = [];

  const rate = ((passed / TEST_CASES.length) * 100).toFixed(1);

  console.log("\n=== é©—è­‰çµæœ ===");
  console.log(`æª¢å‡ºç‡: ${rate}%`);
  console.log(`é€šé: ${passed}/${TEST_CASES.length}`);
  console.log(`å¤±æ•—: ${failed}/${TEST_CASES.length}`);

  if (failedCases.length > 0) {
    console.log("\n=== å¤±æ•—æ¡ˆä¾‹åˆ†æ ===");
    failedCases.forEach(fc => {
      console.log(`\nâŒ ${fc.name} (${fc.id})`);
      console.log(`  æœŸæœ›: ${fc.expected.join(", ")}`);
      console.log(`  å¯¦éš› Top5: ${fc.actualTop5.join(", ")}`);
      console.log(`  ä½¿ç”¨ç—‡ç‹€: ${fc.symptoms.join(", ")}`);
      console.log(`  ä½¿ç”¨ç•°å¸¸: ${fc.labs.join(", ")}`);
    });
  }

  console.log("\n=== è©³ç´°çµæœ ===");
  console.table(details.map(d => ({
    ID: d.id,
    æ¡ˆä¾‹: d.name,
    ç‹€æ…‹: d.status,
    Top1: d.top3[0] || "-",
    Top2: d.top3[1] || "-",
    Top3: d.top3[2] || "-"
  })));

  return {
    rate: rate + "%",
    passed,
    failed,
    total: TEST_CASES.length,
    failedCases,
    details
  };
}

// è¼”åŠ©å‡½å¼ï¼šåˆ—å‡ºæ‰€æœ‰å¯ç”¨çš„ç—‡ç‹€å’Œå¯¦é©—å®¤ID
function listAvailableIDs() {
  console.log("=== å¯ç”¨ç—‡ç‹€ ID ===");
  const symptoms = DB.symptoms.map(sym => ({
    id: sym.id,
    zh: sym.zhName,
    en: sym.enName
  }));
  console.table(symptoms);

  console.log("\n=== å¯ç”¨å¯¦é©—å®¤ç•°å¸¸ ID ===");
  const labs = LAB_DB.abnormalities.map(l => ({
    id: l.id,
    zh: l.zhName,
    en: l.enName,
    category: l.category
  }));
  console.table(labs);
}

// è‡ªå‹•ç”Ÿæˆçµ±è¨ˆå ±å‘Š
function generateSystemStats() {
  const symCount = DB.symptoms.length;
  const labCount = LAB_DB.abnormalities.length;

  let totalDx = 0;
  let labDxCount = 0;

  DB.symptoms.forEach(sym => {
    if (sym.differentials) totalDx += sym.differentials.length;
  });

  LAB_DB.abnormalities.forEach(lab => {
    if (lab.differentials) labDxCount += lab.differentials.length;
  });

  console.log("=== ç³»çµ±çµ±è¨ˆ ===");
  console.log(`ç—‡ç‹€æ•¸é‡: ${symCount}`);
  console.log(`å¯¦é©—å®¤æŒ‡æ¨™æ•¸é‡: ${labCount}`);
  console.log(`ç—‡ç‹€-ç–¾ç—…é—œè¯æ•¸: ${totalDx}`);
  console.log(`å¯¦é©—å®¤-ç–¾ç—…é—œè¯æ•¸: ${labDxCount}`);
  console.log(`ç—‡ç‹€-å¯¦é©—å®¤é—œè¯è¡¨æ¢ç›®: ${Object.keys(SYMPTOM_LAB_LINKS).length}`);

  return {
    symptoms: symCount,
    labs: labCount,
    symptomDxLinks: totalDx,
    labDxLinks: labDxCount,
    symptomLabLinks: Object.keys(SYMPTOM_LAB_LINKS).length
  };
}

console.log("é©—è­‰ç³»çµ±å·²è¼‰å…¥ã€‚åœ¨æ§åˆ¶å°åŸ·è¡Œ runValidation() æ¸¬è©¦æª¢å‡ºç‡ã€‚");
</script>
</body>
</html>
