  }

  const dxMap = {};

  // æ”¶é›†ç—‡ç‹€ç›¸é—œçš„ç–¾ç—…
  state.compositeSymptoms.forEach(symId => {
    const sym = findSymptom(symId);
    if (!sym || !sym.differentials) return;
    sym.differentials.forEach(d => {
      const key = d.en.toLowerCase();
      if (!dxMap[key]) {
        dxMap[key] = {
          ...d,
          symptomMatches: [],
          labMatches: [],
          linkedLabMatches: []
        };
      }
      dxMap[key].symptomMatches.push(sym.zhName);
    });
  });

  // æ”¶é›†å¯¦é©—å®¤ç›¸é—œçš„ç–¾ç—…
  state.compositeLabs.forEach(labId => {
    const lab = LAB_DB.abnormalities.find(x => x.id === labId);
    if (!lab || !lab.differentials) return;
    lab.differentials.forEach(d => {
      const key = d.en.toLowerCase();
      if (!dxMap[key]) {
        dxMap[key] = {
          ...d,
          symptomMatches: [],
          labMatches: [],
          linkedLabMatches: []
        };
      }
      dxMap[key].labMatches.push(lab.zhName);
    });
  });

  // é€é SYMPTOM_LAB_LINKS å¢åŠ é—œè¯åŠ åˆ†
  state.compositeSymptoms.forEach(symId => {
    const linkedLabs = SYMPTOM_LAB_LINKS[symId] || [];
    linkedLabs.forEach(labId => {
      if (state.compositeLabs.includes(labId)) {
        const lab = LAB_DB.abnormalities.find(x => x.id === labId);
        // æ‰¾åˆ°æ‰€æœ‰æœ‰é€™å€‹ lab çš„ç–¾ç—…ï¼Œçµ¦äºˆé—œè¯åŠ åˆ†
        Object.values(dxMap).forEach(dx => {
          if (dx.labMatches.includes(lab?.zhName)) {
            dx.linkedLabMatches.push({ symptom: symId, lab: labId });
          }
        });
      }
    });
  });

  // è¨ˆç®—è¤‡åˆè©•åˆ†
  const results = Object.values(dxMap).map(dx => {
    // éæ¿¾ç‰©ç¨®
    if (speciesFilter !== "all") {
      if (dx.species !== "both" && dx.species !== speciesFilter) return null;
    }

    const symCount = state.compositeSymptoms.length || 1;
    const labCount = state.compositeLabs.length || 1;

    const symScore = dx.symptomMatches.length / symCount;
    const labScore = dx.labMatches.length / labCount;

    let score;
    if (state.compositeSymptoms.length > 0 && state.compositeLabs.length > 0) {
      // å…©è€…éƒ½æœ‰é¸æ“‡æ™‚ä½¿ç”¨åŠ æ¬Š
      score = symScore * SYMPTOM_WEIGHT + labScore * LAB_WEIGHT;
      // é›™é‡è­‰æ“šåŠ æˆ
      if (dx.symptomMatches.length > 0 && dx.labMatches.length > 0) {
        score *= 1.2;
      }
      // é—œè¯åŠ æˆ
      if (dx.linkedLabMatches.length > 0) {
        score *= (1 + 0.1 * Math.min(dx.linkedLabMatches.length, 3));
      }
    } else if (state.compositeSymptoms.length > 0) {
      score = symScore;
    } else {
      score = labScore;
    }

    return { ...dx, score: Math.min(score, 1.0) };
  }).filter(Boolean);

  // æŒ‰åˆ†æ•¸æ’åº
  results.sort((a, b) => b.score - a.score);
  return results;
}

